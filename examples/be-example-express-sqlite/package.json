{
  "name": "be-example-express-sqlite",
  "version": "0.0.13",
  "private": true,
  "scripts": {
    "dev": "nodemon src/server-demo.ts",
    "dev-sse": "nodemon src/server-sse-demo.ts",
    "migrate-sqlite": "npx convostack-storage-engine-prisma-sqlite migrate --dir ./sqlite-storage --db-url \"file:./dev.db\"",
    "studio-sqlite": "npx convostack-storage-engine-prisma-sqlite studio --dir ./sqlite-storage --db-url \"file:./dev.db\"",
    "migrate-postgres": "npx convostack-storage-engine-prisma-postgres migrate --dir ./postgres-storage --db-url \"$DATABASE_URL\" --shadow-db-url \"$SHADOW_DATABASE_URL\"",
    "studio-postgres": "npx convostack-storage-engine-prisma-postgres studio --dir ./postgres-storage --db-url \"$DATABASE_URL\" --shadow-db-url \"$SHADOW_DATABASE_URL\""
  },
  "dependencies": {
    "@types/express": "^4.17.17",
    "convostack": "*",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "nodemon": "^2.0.22"
  }
}
